        {
          "_name": "features9",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@fullHeight) and not (@showTitle) and not (@showSubtitle)": {
              "padding": "0",
              ".card-img, .card-img img, .card-img amp-img, .card-box": {
                "min-height": "450px",
                "@media (max-width: 991px)": {
                  "min-height": "350px"
                }
              },
              ".card-box when not (@showImg)": {
                "@media (min-width: 992px)": {
                  "min-height": "900px"
                }
              },
              ".card-box when not (@showImg) and (@transparentBg) and not (@showBorders)": {
                "min-height": "270px"
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-img when (@showImg)": {
              "padding": "0"
            },
            "& when (@showImg) and (@transparentBg) and not (@showBorders)": {
              ".card-wrapper when (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
                ".card-img": {
                  "@media (max-width: 767px)": {
                    "width": "100%",
                    "margin-bottom": "30px"
                  }
                },
                "@media (min-width: 768px)": {
                  ".card-box": {
                    "margin-left": "15px"
                  },
                  "&.mbr-row-reverse": {
                    ".card-box": {
                      "margin-right": "15px",
                      "margin-left": "0px"
                    }
                  }
                }
              }
            },
            ".card-benefits-box when (@showCardsTitleNumber)": {
              "align-items": "center",
              ".card-number when (@showLineNumber) and (@showCardsNumber)": {
                "position": "relative",
                "padding-left": "70px",
                "&:before": {
                  "content": "''",
                  "width": "50px",
                  "height": "1px",
                  "position": "absolute",
                  "left": "10px",
                  "top": "50%",
                  "transform": "translateY(-50%)",
                  "background-color": "@lineColor"
                }
              }
            },
            ".card-wrapper, .card-wrapper .card-box": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-box when (@showCardsTitleNumber), (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
              "width": "100%",
              "& when (@showBorders), not (@transparentBg)": {
                "padding": "60px",
                "@media (max-width: 991px)": {
                  "padding": "25px"
                }
              }
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-wrapper when (@showCardsEffect)": {
              "& when (@showCardsTitleNumber), (@showCardsTitle)": {
                "& when (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
                  "&:hover when not (@transparentBg), (@showBorders)": {
                    "& when (@showBorders) and (@transparentBg)": {
                      "background-color": "@primaryColor"
                    },
                    "> *, .card-box > *, .card-text-box > *, .card-box .btn[class*=\"-outline\"], .card-benefits-box > *": {
                      "-webkit-transition": "all 0.3s linear",
                      "transition": "all 0.3s linear"
                    },
                    ".mbr-overlay": {
                      "background": "@primaryColor"
                    },
                    ".card-benefits-box > *, .card-text-box > *": {
                      "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                    },
                    ".card-box .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                      "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important",
                      "border-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                    }
                  }
                }
              }
            },
            ".mbr-row .card when not (@showPadding)": {
              "padding": "0",
              "& when (@showBorders)": {
                ".card-wrapper": {
                  "border": "none"
                },
                "@media (min-width: 992px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "1px solid @bordersColor"
                  },
                  "&.lg-pb .card-wrapper when (@cardsAmount > 2)": {
                    "border-bottom": "1px solid @bordersColor"
                  },
                  "&.lg-pb:nth-child(2) .card-wrapper when (@cardsAmount > 2)": {
                    "border-right": "none"
                  }
                },
                "@media (max-width: 991px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "none",
                    "border-bottom": "1px solid @bordersColor"
                  }
                }
              }
            }
          },
          "_customHTML": "<section class=\"features9\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!(fullHeight &amp;&amp; !showTitle &amp;&amp; !showSubtitle)\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!(fullHeight &amp;&amp; !showTitle &amp;&amp; !showSubtitle)\">\n            <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n            <input type=\"checkbox\" title=\"Cards Full Height\" name=\"fullHeight\" condition=\"!showTitle &amp;&amp; !showSubtitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Chess Style\" name=\"chessStyle\" condition=\"showImg\" checked=\"\">\n        <header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n        \n        <header>Cards</header>\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <input type=\"checkbox\" title=\"Show Title Number\" name=\"showCardsTitleNumber\">\n            <input type=\"checkbox\" title=\"Show Number\" name=\"showCardsNumber\" condition=\"showCardsTitleNumber\">\n            <input type=\"checkbox\" title=\"Show Line Before Number\" name=\"showLineNumber\" condition=\"showCardsTitleNumber &amp;&amp; showCardsNumber\" checked=\"\">\n            <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#9ea1a9\" condition=\"showCardsTitleNumber &amp;&amp; showCardsNumber &amp;&amp; showLineNumber\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showCardsSubtitle\">\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\">\n            <select title=\"Align\" name=\"cardsAlign\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n            <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Image Width\" name=\"imgWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"2\" condition=\"showImg\">\n            <input type=\"checkbox\" title=\"Show Effect\" name=\"showCardsEffect\" condition=\"(showCardsTitleNumber || showCardsTitle) &amp;&amp; (showCardsSubtitle || showCardsText || showCardsButtons) &amp;&amp; (showBorders || !transparentBg)\" checked=\"\">\n            <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showImg\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Padding\" name=\"showPadding\" condition=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\" condition=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#7480a5\" condition=\"showBorders &amp;&amp; (showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\n            <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\" checked=\"\">\n            <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#060d3b\" condition=\"!transparentBg &amp;&amp; (showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\n            <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"!transparentBg &amp;&amp; (showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\n        \n        <header>Background</header>\n            <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\" checked=\"\"> \n            <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n            <fieldset type=\"background\" name=\"bg\">\n                <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background9.jpg\">\n                <input type=\"color\" title=\"Background Color\" value=\"#24273a\" selected=\"\">\n            </fieldset>\n            <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#0b2096\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n            <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n            <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n            <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'mbr-px-2': fullWidth,'align-left': titleAlign == 1, 'align-center': titleAlign == 2, 'align-right': titleAlign == 3}\">    \n            <h2 class=\"mbr-section-title mbr-semibold mbr-fonts-style\" mbr-class=\"{'mbr-pb-1':showSubtitle}\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">\n                Features with Images\n            </h2>\n            <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">\n                Features with Images Subtitle\n            </h3>\n        </div>\n        <div class=\"mbr-row mbr-jc-c\" mbr-if=\"showCardsTitleNumbery || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\" mbr-class=\"{'mbr-px-2': fullWidth || !showPadding, 'cardsAmount2': showPadding}\">\n            <div class=\"card mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 mbr-col-xl-6\" mbr-class=\"{'last-child': cardsAmount == 1,\n                                          'md-pb lg-pb': cardsAmount > 2}\">\n                <div class=\"card-wrapper mbr-flex flew-wrap\" mbr-class=\"{'mbr-row-reverse': showImg &amp;&amp; ((!chessStyle &amp;&amp; reverseContent)||(chessStyle &amp;&amp; !reverseContent))}\">\n                    <div class=\"mbr-overlay\" mbr-if=\"!transparentBg\"></div>\n                    <div class=\"card-img\" mbr-if=\"showImg\" mbr-class=\"{'mbr-col-md-4': imgWidth == 1,\n                                     'mbr-col-md-5': imgWidth == 2,\n                                     'mbr-col-md-6': imgWidth == 3,\n                                     'mbr-col-md-7': imgWidth == 4,\n                                     'mbr-col-md-8': imgWidth == 5}\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/operations-driven-design-720x480.jpg\" layout=\"responsive\" width=\"675\" class=\"mobirise-loader\" height=\"450\" alt=\"Mobirise\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/operations-driven-design-720x480.jpg\" alt=\"Mobirise\" title=\"\">\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box mbr-flex mbr-column mbr-jc-c\" mbr-if=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3, 'mbr-px-2':!showBorders &amp;&amp; transparentBg &amp;&amp; !showPadding}\">\n                        <div class=\"card-benefits-box mbr-flex flex-wrap\" mbr-if=\"showCardsTitleNumber\">\n                            <h4 class=\"card-title-number mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitleNumber\" data-app-selector=\".card-title-number\"></h4>\n                            <h4 class=\"card-number mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsNumber\" data-app-selector=\".card-number\"></h4>\n                        </div>\n                        <div class=\"card-text-box mbr-white\" mbr-if=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'mbr-pt-1':showCardsTitleNumber}\">\n                            <h4 class=\"card-subtitle mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsSubtitle\" data-app-selector=\".card-subtitle\">\n                                Lorem ipsum dolor sit amet</h4>\n                            <h3 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pt-1':showCardsSubtitle}\">CLUB OPERATIONS</h3>\n                            <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".mbr-text\" mbr-class=\"{'mbr-pt-4':showCardsTitle || showCardsSubtitle}\">OPERATING CLUBS IS SECOND NATURE TO OUR TEAM, THERE IS NO CLUB TOO BIG OR TOO SMALL THAT WE CANT HANDLE. WE OPERATE EVERY ASPECT FROM OPEN TO CLOSE IN ANY DEPARTMENT. &nbsp;</p>\n                            <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\" mbr-class=\"{'mbr-pt-3':showCardsText || showCardsSubtitle || showCardsTitle}\"><a class=\"btn btn-md btn-primary-outline\" href=\"https://mobirise.com\" data-app-placeholder=\"Type Text\">\n                                    READ MORE</a></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 mbr-col-xl-6\" mbr-class=\"{'last-child': cardsAmount == 2,\n                                          'md-pb lg-pb': cardsAmount > 2}\" mbr-if=\"cardsAmount > 1\">\n                <div class=\"card-wrapper mbr-flex flew-wrap\" mbr-class=\"{'mbr-row-reverse': showImg &amp;&amp; ((!chessStyle &amp;&amp; reverseContent)||(chessStyle &amp;&amp; !reverseContent))}\">\n                    <div class=\"mbr-overlay\" mbr-if=\"!transparentBg\"></div>\n                        <div class=\"card-img\" mbr-if=\"showImg\" mbr-class=\"{'mbr-col-md-4': imgWidth == 1,\n                                     'mbr-col-md-5': imgWidth == 2,\n                                     'mbr-col-md-6': imgWidth == 3,\n                                     'mbr-col-md-7': imgWidth == 4,\n                                     'mbr-col-md-8': imgWidth == 5}\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/training-bar-hospitality-staff-720x308.jpg\" layout=\"responsive\" width=\"1051.948051948052\" class=\"mobirise-loader\" height=\"450\" alt=\"Mobirise\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/training-bar-hospitality-staff-720x308.jpg\" alt=\"Mobirise\" title=\"\">\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box mbr-flex mbr-column mbr-jc-c\" mbr-if=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3, 'mbr-px-2':!showBorders &amp;&amp; transparentBg &amp;&amp; !showPadding}\">\n                        <div class=\"card-benefits-box mbr-flex flex-wrap\" mbr-if=\"showCardsTitleNumber\">\n                            <h4 class=\"card-title-number mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitleNumber\" data-app-selector=\".card-title-number\"></h4>\n                            <h4 class=\"card-number mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsNumber\" data-app-selector=\".card-number\">2</h4>\n                        </div>\n                        <div class=\"card-text-box mbr-white\" mbr-if=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'mbr-pt-1':showCardsTitleNumber}\">\n                            <h4 class=\"card-subtitle mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsSubtitle\" data-app-selector=\".card-subtitle\">\n                                Lorem ipsum dolor sit amet</h4>\n                            <h3 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pt-1':showCardsSubtitle}\">STAFF TRAINING</h3>\n                            <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".mbr-text\" mbr-class=\"{'mbr-pt-4':showCardsTitle || showCardsSubtitle}\">\n                               WITH EXPERIENCE IN ALL POSITIONS WE CAN TRAIN STAFF IN ANY POSITION FROM AS SIMPLE AS A BATHROOM ATTENDANT TO AS DEMANDING AS A CLUB MANAGER. WE'VE GOT IT COVERED.</p>\n                            <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\" mbr-class=\"{'mbr-pt-3':showCardsText || showCardsSubtitle || showCardsTitle}\"><a class=\"btn btn-md btn-primary-outline\" href=\"https://mobirise.com\" data-app-placeholder=\"Type Text\">\n                                    READ MORE</a></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 mbr-col-xl-6\" mbr-class=\"{'last-child': cardsAmount == 3}\" mbr-if=\"cardsAmount > 2\">\n                <div class=\"card-wrapper mbr-flex flew-wrap\" mbr-class=\"{'mbr-row-reverse': showImg &amp;&amp; reverseContent}\">\n                    <div class=\"mbr-overlay\" mbr-if=\"!transparentBg\"></div>\n                    <div class=\"card-img\" mbr-if=\"showImg\" mbr-class=\"{'mbr-col-md-4': imgWidth == 1,\n                                     'mbr-col-md-5': imgWidth == 2,\n                                     'mbr-col-md-6': imgWidth == 3,\n                                     'mbr-col-md-7': imgWidth == 4,\n                                     'mbr-col-md-8': imgWidth == 5}\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/security-article-bb17-2016-billboard-1548-636x421.jpg\" layout=\"responsive\" width=\"679.8099762470308\" class=\"mobirise-loader\" height=\"450\" alt=\"Mobirise\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/security-article-bb17-2016-billboard-1548-636x421.jpg\" alt=\"Mobirise\" title=\"\">\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box mbr-flex mbr-column mbr-jc-c\" mbr-if=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3, 'mbr-px-2':!showBorders &amp;&amp; transparentBg &amp;&amp; !showPadding}\">\n                        <div class=\"card-benefits-box mbr-flex flex-wrap\" mbr-if=\"showCardsTitleNumber\">\n                            <h4 class=\"card-title-number mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitleNumber\" data-app-selector=\".card-title-number\"></h4>\n                            <h4 class=\"card-number mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsNumber\" data-app-selector=\".card-number\">3</h4>\n                        </div>\n                        <div class=\"card-text-box mbr-white\" mbr-if=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'mbr-pt-1':showCardsTitleNumber}\">\n                            <h4 class=\"card-subtitle mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsSubtitle\" data-app-selector=\".card-subtitle\">\n                                Lorem ipsum dolor sit amet</h4>\n                            <h3 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pt-1':showCardsSubtitle}\">\n                                SECURITY TRAINING</h3>\n                            <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".mbr-text\" mbr-class=\"{'mbr-pt-4':showCardsTitle || showCardsSubtitle}\">\n                               SECURITY IS IMPORTANT IN ANY NIGHT CLUB SITUATION. ITS YOUR FIRST AND LAST LINE OF DEFENSE. WE SPECIALIZE IN ARMED AND UNARMED SECURITY SERVICES. WE OFFER TRAINING AND SUPERVION SERVICES. WE ARE LICENSE AND INSURED.</p>\n                            <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\" mbr-class=\"{'mbr-pt-3':showCardsText || showCardsSubtitle || showCardsTitle}\"><a class=\"btn btn-md btn-primary-outline\" href=\"https://mobirise.com\" data-app-placeholder=\"Type Text\">\n                                    READ MORE</a></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 mbr-col-xl-6\" mbr-class=\"{'last-child': cardsAmount == 4}\" mbr-if=\"cardsAmount > 3\">\n                <div class=\"card-wrapper mbr-flex flew-wrap\" mbr-class=\"{'mbr-row-reverse': showImg &amp;&amp; reverseContent}\">\n                    <div class=\"mbr-overlay\" mbr-if=\"!transparentBg\"></div>\n                    <div class=\"card-img\" mbr-if=\"showImg\" mbr-class=\"{'mbr-col-md-4': imgWidth == 1,\n                                     'mbr-col-md-5': imgWidth == 2,\n                                     'mbr-col-md-6': imgWidth == 3,\n                                     'mbr-col-md-7': imgWidth == 4,\n                                     'mbr-col-md-8': imgWidth == 5}\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/alcohol-bar-beer-1283219-720x480.jpg\" layout=\"responsive\" width=\"675\" class=\"mobirise-loader\" height=\"450\" alt=\"Mobirise\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/alcohol-bar-beer-1283219-720x480.jpg\" alt=\"Mobirise\" title=\"\">\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box mbr-flex mbr-column mbr-jc-c\" mbr-if=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3, 'mbr-px-2':!showBorders &amp;&amp; transparentBg &amp;&amp; !showPadding}\">\n                        <div class=\"card-benefits-box mbr-flex flex-wrap\" mbr-if=\"showCardsTitleNumber\" mbr-class=\"{'mbr-pb-1':showCardsTitle}\">\n                            <h4 class=\"card-title-number mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitleNumber\" data-app-selector=\".card-title-number\"></h4>\n                            <h4 class=\"card-number mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsNumber\" data-app-selector=\".card-number\">4</h4>\n                        </div>\n                        <div class=\"card-text-box mbr-white\" mbr-if=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'mbr-pt-1':showCardsTitleNumber}\">\n                            <h4 class=\"card-subtitle mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsSubtitle\" data-app-selector=\".card-subtitle\">\n                                Lorem ipsum dolor sit amet</h4>\n                            <h3 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pt-1':showCardsSubtitle}\">\n                                BAR SPOTTING</h3>\n                            <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".mbr-text\" mbr-class=\"{'mbr-pt-4':showCardsTitle || showCardsSubtitle}\">\n                               WHEATHER WE'D LIKE TO ADMIT OR NOT, STEALING IS A MAJOR CONCERN WHEN IT COMES TO NIGHT CLUBS AND BARS. WE HAVE A TEAM SPEACIALIZED IN SPOTTING SIGNS OF THEFT AND WILL FIND WHOEVER OR WHAT EVER IS CAUSING YOUR BAR SALES TO DROP.</p>\n                            <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\" mbr-class=\"{'mbr-pt-3':showCardsText || showCardsSubtitle || showCardsTitle}\"><a class=\"btn btn-md btn-primary-outline\" href=\"https://mobirise.com\" data-app-placeholder=\"Type Text\">\n                                    READ MORE</a></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
